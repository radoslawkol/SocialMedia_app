{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": "warn",
		"linebreak-style": 0,
		"react/prop-types": "off",
		"no-empty": "warn",
		"no-mixed-spaces-and-tabs": 0
	}
}
